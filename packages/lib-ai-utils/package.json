{
    "name": "@onecoach/lib-ai-utils",
    "version": "0.0.0",
    "description": "Shared AI utilities for CoachOne backend",
    "main": "src/index.ts",
    "types": "src/index.ts",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "default": "./src/index.ts"
        },
        "./json-parser": {
            "types": "./src/json-parser.ts",
            "default": "./src/json-parser.ts"
        },
        "./model-factory": {
            "types": "./src/model-factory.ts",
            "default": "./src/model-factory.ts"
        },
        "./response": {
            "types": "./src/response.ts",
            "default": "./src/response.ts"
        },
        "./constants": {
            "types": "./src/constants.ts",
            "default": "./src/constants.ts"
        },
        "./schemas/*": {
            "types": "./src/schemas/*.ts",
            "default": "./src/schemas/*.ts"
        }
    },
    "scripts": {
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@ai-sdk/openai": "3.0.0-beta.74",
        "ai": "6.0.0-beta.159"
    },
    "devDependencies": {
        "typescript": "^5.9.3"
    },
    "sideEffects": false
}