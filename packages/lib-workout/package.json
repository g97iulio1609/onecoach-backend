{
  "name": "@onecoach/lib-workout",
  "version": "0.0.0",
  "description": "Servizi per il dominio workout",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./program-transform": {
      "types": "./src/helpers/program-transform.ts",
      "default": "./src/helpers/program-transform.ts"
    },
    "./intensity-calculator": {
      "types": "./src/helpers/intensity-calculator.ts",
      "default": "./src/helpers/intensity-calculator.ts"
    },
    "./weight-calculator": {
      "types": "./src/workout-weight-calculator.service.ts",
      "default": "./src/workout-weight-calculator.service.ts"
    },
    "./workout.service": {
      "types": "./src/workout.service.ts",
      "default": "./src/workout.service.ts"
    },
    "./helpers/*": {
      "types": "./src/helpers/*.ts",
      "default": "./src/helpers/*.ts"
    },
    "./exercise-builder": {
      "types": "./src/helpers/exercise-builder.ts",
      "default": "./src/helpers/exercise-builder.ts"
    },
    "./normalizers/workout-normalizer": {
      "types": "./src/helpers/normalizers/workout-normalizer.ts",
      "default": "./src/helpers/normalizers/workout-normalizer.ts"
    },
    "./program-server-transform": {
      "types": "./src/helpers/program-server-transform.ts",
      "default": "./src/helpers/program-server-transform.ts"
    },
    "./program-helpers": {
      "types": "./src/helpers/program-transform.ts",
      "default": "./src/helpers/program-transform.ts"
    },
    "./utils/progression-calculator": {
      "types": "./src/helpers/utils/progression-calculator.ts",
      "default": "./src/helpers/utils/progression-calculator.ts"
    },
    "./utils/type-helpers": {
      "types": "./src/helpers/utils/type-helpers.ts",
      "default": "./src/helpers/utils/type-helpers.ts"
    },
    "./workout-operations": {
      "types": "./src/helpers/workout-operations.ts",
      "default": "./src/helpers/workout-operations.ts"
    },
    "./workout-weight-calculator.service": {
      "types": "./src/workout-weight-calculator.service.ts",
      "default": "./src/workout-weight-calculator.service.ts"
    },
    "./workout-template.service": {
      "types": "./src/workout-template.service.ts",
      "default": "./src/workout-template.service.ts"
    },
    "./workout-tracking.service": {
      "types": "./src/workout-tracking.service.ts",
      "default": "./src/workout-tracking.service.ts"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@onecoach/types": "workspace:*",
    "@onecoach/schemas": "workspace:*",
    "@onecoach/constants": "workspace:*",
    "@onecoach/contracts": "workspace:*",
    "@onecoach/lib-core": "workspace:*",
    "@onecoach/lib-import-core": "workspace:*",
    "@onecoach/lib-shared": "workspace:*",
    "@onecoach/lib-ai": "workspace:*",
    "@onecoach/lib-ai-agents": "workspace:*",
    "@prisma/client": "7.1.0",
    "ai": "6.0.0-beta.124",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3"
  },
  "sideEffects": false
}
