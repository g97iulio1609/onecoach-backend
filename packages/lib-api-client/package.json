{
  "name": "@onecoach/lib-api-client",
  "version": "0.0.0",
  "description": "API client (core, hooks, queries, react-query) per web e native",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./hooks": {
      "types": "./src/hooks/index.ts",
      "default": "./src/hooks/index.ts"
    },
    "./hooks/*": {
      "types": "./src/hooks/*.ts",
      "default": "./src/hooks/*.ts"
    },
    "./react-query": {
      "types": "./src/react-query/index.ts",
      "default": "./src/react-query/index.ts"
    },
    "./react-query/*": {
      "types": "./src/react-query/*.ts",
      "default": "./src/react-query/*.ts"
    },
    "./queries": {
      "types": "./src/queries/index.ts",
      "default": "./src/queries/index.ts"
    },
    "./queries/*": {
      "types": "./src/queries/*.ts",
      "default": "./src/queries/*.ts"
    },
    "./memory": {
      "types": "./src/memory.ts",
      "default": "./src/memory.ts"
    },
    "./native": {
      "types": "./src/native.ts",
      "default": "./src/native.ts"
    }
  },
  "scripts": {
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@onecoach/lib-core": "workspace:*",
    "@onecoach/lib-marketplace": "workspace:*",
    "@onecoach/lib-shared": "workspace:*",
    "@onecoach/lib-stores": "workspace:*",
    "@tanstack/react-query": "^5.90.16",
    "react": "^19.2.3",
    "@onecoach/types-core": "workspace:*",
    "@onecoach/types-workout": "workspace:*",
    "@onecoach/types-nutrition": "workspace:*",
    "@onecoach/types-chat": "workspace:*",
    "@onecoach/types-analytics": "workspace:*",
    "@onecoach/types-ai": "workspace:*",
    "@onecoach/types-ui": "workspace:*",
    "@onecoach/types-domain": "workspace:*",
    "@onecoach/types-database": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^18.3.1"
  },
  "sideEffects": false
}
